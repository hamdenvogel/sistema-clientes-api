	<parameter name="ID_PRESTADOR" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select 
	p.id, 
	p.nome,
	p.data_cadastro,
	p.cpf, 
	p.pix  
from meusservicos.prestador p
where 
 ( p.id = $P{ID_PRESTADOR} or $P{ID_PRESTADOR} is null )
 and p.data_cadastro between $P{DATA_INICIO} and $P{DATA_FIM}
order by p.nome, p.data_cadastro asc]]>
