	<queryString>
		<![CDATA[select 
distinct to_char(sp.data, 'MM-YYYY') as mes_ano,
count(case sp.status 
when 'E' then 'Em Atendimento'
else null
end) as em_atendimento,
count(case sp.status 
when 'F' then 'Finalizado'
else null
end) as finalizado,
count(case sp.status 
when 'C' then 'Cancelado'
else null
end) as cancelado
from meusservicos.servicoprestado sp
where sp.status in ('E', 'F', 'C')
and sp.data is not null
and sp.data between $P{DATA_INICIO} and $P{DATA_FIM}
group by to_char(sp.data, 'MM-YYYY')
order by to_char(sp.data, 'MM-YYYY')]]>
	</queryString>
